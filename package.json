{
    "private": true,
    "author": "Yomua",
    "scripts": {
        "start": "yarn build-article && umi dev ",
        "build": "umi build",
        "build-article": "ts-node src/scripts/index.ts",
        "postinstall": "umi generate tmp",
        "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
        "test": "umi-test",
        "test:coverage": "umi-test --coverage",
        "prepare": "husky install",
        "eslint": "npx eslint src/",
        "lint-staged": "npx lint-staged src",
        "umi-webpack": "yarn umi-webpack-dev && yarn umi-webpack-prod",
        "umi-webpack-dev": "umi webpack > umi_webpack_dev.js",
        "umi-webpack-prod": "set NODE_ENV=production && umi webpack > umi_webpack_prod.js"
    },
    "gitHooks": {
        "pre-commit": "lint-staged"
    },
    "lint-staged": {
        "*.{js,ts,tsx}": [
            "eslint",
            "prettier --write",
            "git add"
        ]
    },
    "dependencies": {
        "@fortawesome/fontawesome-free": "^6.3.0",
        "@fortawesome/fontawesome-svg-core": "^6.3.0",
        "@fortawesome/free-brands-svg-icons": "^6.3.0",
        "@fortawesome/free-regular-svg-icons": "^6.3.0",
        "@fortawesome/free-solid-svg-icons": "^6.3.0",
        "@fortawesome/react-fontawesome": "^0.2.0",
        "@yomua/y-classnames": "^0.0.2",
        "@yomua/y-eventemitter": "^0.0.2",
        "@yomua/y-hooks": "^0.0.6",
        "@yomua/y-indexeddb": "^0.0.6",
        "@yomua/y-screw": "^0.1.1",
        "@yomua/y-tlog": "^0.1.6",
        "antd": "^5.12.7",
        "mobx": "^6.8.0",
        "mobx-react": "^7.6.0",
        "openai": "^3.1.0",
        "react": "^18.2.0",
        "react-custom-scrollbars-2": "^4.5.0",
        "react-dom": "^18.2.0",
        "react-markdown": "^8.0.7",
        "react-router-dom": "^6.21.1",
        "react-syntax-highlighter": "^15.5.0",
        "rehype-raw": "^6.1.1",
        "remark-gfm": "^3.0.1",
        "three": "^0.155.0",
        "umi": "^3.5.13"
    },
    "devDependencies": {
        "@types/node": "^18.15.0",
        "@types/react": "^18.0.28",
        "@types/react-dom": "^18.0.11",
        "@types/three": "^0.155.0",
        "@typescript-eslint/eslint-plugin": "^5.52.0",
        "@typescript-eslint/parser": "^5.52.0",
        "@umijs/test": "^3.5.13",
        "cross-env": "^7.0.3",
        "dotenv": "^16.3.1",
        "eslint": "^8.34.0",
        "eslint-config-airbnb": "^19.0.4",
        "eslint-plugin-import": "^2.25.4",
        "eslint-plugin-jsx-a11y": "^6.5.1",
        "eslint-plugin-react": "^7.29.0",
        "eslint-plugin-react-hooks": "^4.3.0",
        "eslint-plugin-react-native": "^4.0.0",
        "fs-extra": "^11.2.0",
        "html-loader": "^4.2.0",
        "husky": "^7.0.4",
        "lint-staged": "^10.0.7",
        "markdown-loader": "^8.0.0",
        "prettier": "^2.2.0",
        "ts-node": "10.6.0",
        "typescript": "^4.9.5"
    }
}
