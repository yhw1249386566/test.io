{
  "private": true,
  "scripts": {
    "start-dev": "umi dev",
    "build": "umi build",
    "postinstall": "umi generate tmp",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@ant-design/pro-layout": "^6.5.0",
    "markdown": "^0.5.0",
    "mobx": "^6.3.2",
    "mobx-react": "^7.2.0",
    "react": "17.x",
    "react-dom": "17.x",
    "umi": "^3.5.13"
  },
  "devDependencies": {
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@umijs/plugin-sass": "^1.1.1",
    "@umijs/preset-react": "1.x",
    "@umijs/test": "^3.5.13",
    "lint-staged": "^10.0.7",
    "node-sass": "4.14.0",
    "prettier": "^2.2.0",
    "sass-loader": "^12.1.0",
    "typescript": "^4.1.2"
  }
}
